    printf("%f\n", zmienna +1);




    return 0;
}

Zadanie 1.2.8

int main()
{
    float zmienna1;
    float zmienna2;
    float zmienna3;

    printf("Podaj 3 wartosci: \n");
    scanf("%f", &zmienna1);
    scanf("%f", &zmienna2);
    scanf("%f", &zmienna3);

    float srednia = (zmienna1+zmienna2+zmienna3)/3;

    printf("%f\n", srednia);




    return 0;
}


Zadanie 1.2.9

int main()
{
int liczba;


    printf("Podaj 1 wartosc: \n");
    scanf("%d", &liczba);

    int pierwiastek = sqrt(liczba);




    printf("%d\n", pierwiastek);




    return 0;
}


