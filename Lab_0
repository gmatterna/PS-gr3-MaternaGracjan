Zadanie 1

int main()
{
    int styczen = 31;
    int luty = 29;
    int marzec = 31;

    int iloscdni = styczen+luty+marzec;



    printf("%d", iloscdni);
    return 0;
}


Zadanie 2

int main()
{
    int suma =0;
    for(int i =0;i<=10;i++)
    {
        suma = suma + i;

    }



    printf("%d", suma);
    return 0;
}

Zadanie 3

int main()
{
    int iloczyn = 1;
    for(int i =1;i<=10;i++)
    {
        iloczyn*=i;

    }



    printf("%d", iloczyn);
    return 0;
}

Zadanie 4

int main()
{
    float kwota = 1000;

    float oproc = 1.06;

    float pierwszy = kwota*oproc;
    printf("%f,  - Kwota po pierwszym roku\n", pierwszy);

    float drugi = pierwszy*oproc;
    printf("%f, - Kwota po drugim roku\n",drugi);

    float trzeci = drugi*oproc;
    printf("%f, - Kwota po trzecim roku\n",trzeci);






    return 0;
}

