{
    float zmienna;
    printf("Podaj wartosc zmiennej\n");
    scanf("%f", &zmienna);
    printf("Zmienna podana przez uzytnkowika ma wartosc:%f\n", zmienna);




    return 0;
}

Zadanie 1.2.6

int main()
{
    float zmienna1;
    float zmienna2;
    float zmienna3;
    printf("Podaj 3 wartosci: \n");
    scanf("%f", &zmienna1);
    scanf("%f", &zmienna2);
    scanf("%f", &zmienna3);

    printf("%f\n", zmienna1);
    printf("%f\n", zmienna2);
    printf("%f", zmienna3);





    return 0;
}

Zadanie 1.2.7

